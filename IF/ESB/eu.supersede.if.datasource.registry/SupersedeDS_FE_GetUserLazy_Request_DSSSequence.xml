<?xml version="1.0" encoding="UTF-8"?>
<sequence name="SupersedeDS_FE_GetUserLazy_Request_DSSSequence"
  onError="conf:/sendErrorMessage.xml" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
  <payloadFactory media-type="xml">
    <format>
      <p:select_with_key_users_lazy_operation xmlns:p="http://schema.authentication.supersede.eu">
        <xs:user_id xmlns:xs="http://schema.supersede.eu">$1</xs:user_id>
      </p:select_with_key_users_lazy_operation>
    </format>
    <args>
      <arg evaluator="xml" expression="$ctx:id_user"/>
    </args>
  </payloadFactory>
</sequence>
